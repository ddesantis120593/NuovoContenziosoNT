                <xsd:appinfo> 
                    <ext:label>Converti esadecimale</ext:label> 
                    <ext:group id="Advanced"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
            <xsd:simpleType> 
                <xsd:union memberTypes="variableType"> 
                    <xsd:simpleType> 
                        <xsd:restriction base="xsd:string"> 
