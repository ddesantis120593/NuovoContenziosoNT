                <xsd:appinfo> 
                    <ext:label>Valore della proprietà nell'ambito applicazione</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:anyAttribute processContents="skip"> 
        </xsd:anyAttribute> 
