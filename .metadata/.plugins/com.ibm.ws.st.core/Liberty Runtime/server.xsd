                    <ext:label>Nome della proprietà nell'ambito applicazione</ext:label> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="value" use="optional" default="" type="xsd:string"> 
            <xsd:annotation> 
