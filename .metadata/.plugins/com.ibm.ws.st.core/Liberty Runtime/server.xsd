                    <ext:label>Utilizza aggiornamento blocco</ext:label> 
                    <ext:group id="Advanced"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="useDrdaMetadataVersion" use="optional" type="booleanType"> 
            <xsd:annotation> 
                <xsd:documentation>Proprietà driver JDBC: useDrdaMetadataVersion.</xsd:documentation> 
                <xsd:appinfo> 
                    <ext:label>Utilizza versione metadati DRDA</ext:label> 
                    <ext:group id="Advanced"/> 
                </xsd:appinfo> 
            </xsd:annotation> 
        </xsd:attribute> 
        <xsd:attribute name="user" use="optional" type="xsd:string"> 
